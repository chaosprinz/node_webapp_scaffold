  "description": "Turns every browser in your network into an shortcut-keyboard for your windows-pc.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "./node_modules/.bin/mocha",
    "server": "./node_modules/.bin/supervisor -w app -i app/public index.js",
